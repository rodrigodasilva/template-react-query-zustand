    "storybook": "^7.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.32.0"
  }
}
